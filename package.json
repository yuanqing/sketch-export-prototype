{
  "name": "sketch-export-prototype",
  "version": "0.0.0",
  "description": "A Sketch plugin to export a clickable prototype from your Sketch file",
  "keywords": [
    "plugin",
    "sketch",
    "sketch-plugin-helper"
  ],
  "license": "MIT",
  "author": "Lim Yuan Qing",
  "repository": "yuanqing/sketch-export-prototype",
  "dependencies": {
    "@skpm/fs": "^0.2.4"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "sketch-plugin-helper": "*"
  },
  "scripts": {
    "build": "sph build && cd prototype-viewer && yarn build",
    "fix": "sph lint --fix && cd prototype-viewer && yarn fix",
    "lint": "sph lint && cd prototype-viewer && yarn lint",
    "symlink": "sph symlink",
    "unlink": "sph symlink --delete",
    "version": "sph version",
    "watch": "concurrently 'sph build --watch' 'cd prototype-viewer && yarn watch'"
  },
  "sketch-plugin-helper": {
    "name": "Export Prototype",
    "menu": [
      {
        "handler": "export-prototype",
        "label": "Export Prototype"
      },
      "-",
      {
        "Settings": [
          {
            "handler": "settings/settings",
            "label": "Settings"
          },
          "-",
          {
            "handler": "settings/reset-settings",
            "label": "Reset Settings"
          }
        ]
      }
    ],
    "defaultSettings": {
      "outputDirectoryPath": "~/Desktop/Sketch Prototypes"
    }
  }
}
